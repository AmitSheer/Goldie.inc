<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com.goldie">
<uses-permission android:name="android.permission.INTERNET" />
<application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/Theme.Goldieinc">
    <activity
        android:name=".shop.ShopActivity"
        android:exported="true"
        android:label="@string/app_name">
        <nav-graph android:value="@navigation/nav_graph" />
    </activity>
    <activity
        android:name=".admin.manager.AdminMainActivity"
        android:exported="true"
        android:label="@string/app_name">
        <nav-graph android:value="@navigation/admin_nav_graph" />
    </activity>
    <activity
        android:name=".admin.delivery.DeliveriesAdminMainActivity"
        android:exported="true"
        android:label="@string/app_name">
        <nav-graph android:value="@navigation/delivery_admin_nav" />
    </activity>
    <activity
        android:name=".account.LoginActivity"
        android:exported="true"
        android:label="@string/app_name">
        <nav-graph android:value="@navigation/login_nav" />
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
</application>

</manifest>