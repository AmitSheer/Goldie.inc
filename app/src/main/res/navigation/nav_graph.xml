<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.goldie.nav.HomeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.goldie.login.LoginFragment"
        android:label="Login"
        tools:layout="@layout/fragment_login">
        <action
            android:id="@+id/action_loginFragment_to_welcomeFragment"
            app:destination="@id/welcomeFragment" />
        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
        <action
            android:id="@+id/action_loginFragment_to_adminFragment"
            app:destination="@id/adminFragment" />
    </fragment>
    <fragment
        android:id="@+id/welcomeFragment"
        android:name="com.goldie.nav.WelcomeFragment"
        android:label="Welcom"
        tools:layout="@layout/fragment_welcome">
        <argument
            android:name="password"
            app:argType="string" />
        <argument
            android:name="username"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.goldie.login.RegisterFragment"
        android:label="Register"
        tools:layout="@layout/fragment_register" />
    <fragment
        android:id="@+id/shoppingCartFragment"
        android:name="com.goldie.shoppingcart.ShoppingCartFragment"
        android:label="Shopping Cart"
        tools:layout="@layout/fragment_shopping_cart" >
        <action
            android:id="@+id/action_shoppingCartFragment_to_paymentFragment2"
            app:destination="@id/paymentFragment" />
    </fragment>
    <fragment
        android:id="@+id/paymentFragment"
        android:name="com.goldie.shoppingcart.PaymentFragment"
        android:label="Payment"
        tools:layout="@layout/fragment_payment" />
    <fragment
        android:id="@+id/adminFragment"
        android:name="com.goldie.admin.AdminFragment"
        android:label="Admin Login"
        tools:layout="@layout/fragment_admin_manager" >
        <action
            android:id="@+id/action_adminFragment_to_adminOptionsFragment"
            app:destination="@id/adminOptionsFragment" />
    </fragment>
    <fragment android:id="@+id/storageManagmentFragment" />
    <fragment android:id="@+id/adminOptionsFragment" >
        <action
            android:id="@+id/action_adminOptionsFragment_to_storageManagmentFragment"
            app:destination="@id/storageManagmentFragment" />
        <action
            android:id="@+id/action_adminOptionsFragment_to_ordersManagementFragment"
            app:destination="@id/ordersManagementFragment" />
    </fragment>
    <fragment android:id="@+id/ordersManagementFragment" />
    <activity
        android:id="@+id/menu"
        android:name="com.goldie.menu.menu"
        android:label="activity_menu"
        tools:layout="@layout/activity_menu" />
    <fragment android:id="@+id/sellerAdviceFragment" />
</navigation>